h3 Instances
table
	tr
		//- a(href=`/resetData`)  Delete All Instances Data

table
	tr
		td Name
		td Task Id
		td Requester Name 
		td Requester EMP ID 
		td Started
		td Ended
		td Examine Log
		//- td Delete
	each instance in instances
		- var ref = "/instanceDetails?id="+instance.id;
		- var ref2 = "/deleteInstance?id="+instance.id;
		tr
			td #{instance.name}
			td #{instance.task_id}
			td #{instance.data.requester.name}
			td #{instance.data.requester.empid}
			td #{instance.fromNow}
			td #{instance.endFromNow} 
			td
				a(href=`${ref}`) Examine
